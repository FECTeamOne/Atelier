{
  "plugins": [
    "babel-plugin-styled-components",
    "@babel/syntax-dynamic-import"
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": "auto"
      }
    ],
    [
      "@babel/preset-react",
      {
        "runtime": "automatic"
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": [
        "babel-plugin-styled-components",
        "react-refresh/babel"
      ]
    }
  }
}
